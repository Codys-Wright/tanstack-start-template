{
  "name": "@core",
  "version": "0.0.1",
  "type": "module",
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./database": {
      "types": "./src/database/index.ts",
      "import": "./src/database/index.ts",
      "default": "./src/database/index.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts",
      "default": "./src/*.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@effect-atom/atom": "*",
    "@effect-atom/atom-react": "*",
    "@effect/opentelemetry": "*",
    "@effect/platform": "*",
    "@effect/platform-bun": "*",
    "@effect/rpc": "*",
    "@effect/sql": "*",
    "@effect/sql-pg": "*",
    "effect": "*",
    "pg": "*"
  },
  "devDependencies": {
    "@effect/vitest": "*",
    "@testcontainers/postgresql": "*",
    "@types/pg": "*",
    "typescript": "*",
    "vite": "*",
    "vitest": "*"
  }
}
