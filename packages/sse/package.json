{
  "name": "@sse",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./server": {
      "types": "./src/server.ts",
      "import": "./src/server.ts",
      "default": "./src/server.ts"
    },
    "./client": {
      "types": "./src/client.ts",
      "import": "./src/client.ts",
      "default": "./src/client.ts"
    },
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts",
      "default": "./src/*.ts"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@effect-atom/atom": "*",
    "@effect-atom/atom-react": "*",
    "effect": "*"
  },
  "devDependencies": {
    "@types/node": "*"
  }
}
