{
  "name": "@my-artist-type",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "effect-language-service patch",
    "dev": "bun --bun vite dev --port 3000",
    "dev:experiment": "dotenv -- tsx ./src/features/experiment/server.ts",
    "build": "bun --bun vite build",
    "preview": "bun --bun vite preview",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "db:migrate": "dotenv -- tsx ./src/features/core/database/scripts/migrator.ts",
    "db:reset": "dotenv -- tsx ./src/features/core/database/scripts/reset-database.ts",
    "db:add-fake-column": "dotenv -- bun run ../../packages/auth/src/database/scripts/add-fake-column.ts",
    "db:seed": "dotenv -- bun run ../../packages/auth/src/database/scripts/seed-users.ts",
    "db:clean": "dotenv -- bun run ../../packages/auth/src/database/scripts/cleanup-fake-users.ts",
    "admin:set": "dotenv -- bun run ../../packages/auth/src/database/scripts/make-admin.ts",
    "auth:migrate": "dotenv -- pnpx @better-auth/cli migrate --config ../../packages/auth/src/_core/auth.ts",
    "auth:generate": "dotenv -- pnpx @better-auth/cli generate --config ../../packages/auth/src/_core/auth.ts --output ../../packages/auth/src/database/migrations/better-auth-schema.sql -y"
  },
