{
  "name": "my-artist-type",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "effect-language-service patch",
    "dev": "vite dev --port 3000",
    "dev:experiment": "nx exec -- dotenv -- tsx ./src/features/experiment/server.ts",
    "build": "nx exec -- vite build",
    "preview": "vite preview",
    "test": "nx exec -- vitest",
    "typecheck": "nx exec -- tsc --noEmit",
    "db:migrate": "nx exec -- dotenv -- tsx ./src/features/core/database/scripts/migrator.ts",
    "db:reset": "nx exec -- dotenv -- tsx ./src/features/core/database/scripts/reset-database.ts",
    "db:add-fake-column": "nx exec -- dotenv -- bun run ./src/features/auth/database/scripts/add-fake-column.ts",
    "db:seed": "nx exec -- dotenv -- bun run ./src/features/auth/database/scripts/seed-users.ts",
    "db:clean": "nx exec -- dotenv -- bun run ./src/features/auth/database/scripts/cleanup-fake-users.ts",
    "admin:set": "nx exec -- dotenv -- bun run ./src/features/auth/database/scripts/make-admin.ts",
    "auth:migrate": "nx exec -- dotenv -- pnpx @better-auth/cli migrate --config ./src/features/auth/auth.ts",
    "auth:generate": "nx exec -- dotenv -- pnpx @better-auth/cli generate --config ./src/features/auth/auth.ts --output ./src/features/auth/database/migrations/better-auth-schema.sql -y"
  },
