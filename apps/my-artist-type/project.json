{
  "name": "my-artist-type",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/my-artist-type/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/my-artist-type"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nx/vite:serve",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "my-artist-type:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "my-artist-type:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "my-artist-type:build:production"
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview",
      "options": {
        "buildTarget": "my-artist-type:build"
      }
    },
    "test": {
      "executor": "@nx/vite:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/apps/my-artist-type"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "typecheck": {
      "executor": "@nx/js:tsc",
      "options": {
        "tsConfig": "apps/my-artist-type/tsconfig.app.json"
      },
      "outputs": ["{options.outputPath}"],
      "outputPath": "apps/my-artist-type/dist/out-tsc"
    }
  }
}